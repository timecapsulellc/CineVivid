# Copy this file to .env and fill in your actual API keys
# NEVER commit .env to version control

# Hugging Face API Token (required for SkyReels-V2 model access)
# Get from: https://huggingface.co/settings/tokens
HUGGINGFACE_TOKEN=hf_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ElevenLabs API Key (required for voiceover generation)
# Get from: https://elevenlabs.io/app/profile
ELEVENLABS_API_KEY=sk_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Redis Configuration (optional, defaults to localhost)
REDIS_URL=redis://localhost:6379/0

# Backend Configuration
BACKEND_HOST=localhost
BACKEND_PORT=8001

# Frontend Configuration
FRONTEND_HOST=localhost
FRONTEND_PORT=1234